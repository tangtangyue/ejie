<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd"> 

<configuration>
	
	<!-- 实体类,简称 -设置别名 -->
	<typeAliases>
		<typeAlias alias="GlyEntity" type="com.stx.entity.GlyEntity" />
		<typeAlias alias="MtypeEntity" type="com.stx.entity.MtypeEntity" />
		<typeAlias alias="MenuEntity" type="com.stx.entity.MenuEntity" />
		<typeAlias alias="PuserEntity" type="com.stx.entity.PuserEntity" />
		<typeAlias alias="OrdersEntity" type="com.stx.entity.OrdersEntity" />
		<typeAlias alias="OdetailEntity" type="com.stx.entity.OdetailEntity" />
		
		<!--
		<typeAlias alias="Person" type="com.stx.model.Person" />
		<typeAlias alias="TeacherModel" type="com.stx.model.TeacherModel" />
		-->
	</typeAliases>
	<!-- 实体接口映射资源 -->
	<!--
		说明：如果xxMapper.xml配置文件放在和xxMapper.java统一目录下，mappers也可以省略，因为org.mybatis.spring.mapper.MapperFactoryBean默认会去查找与xxMapper.java相同目录和名称的xxMapper.xml
	-->
	<mappers>
		<mapper resource="com/stx/mapper/glyMapper.xml" />	
		<mapper resource="com/stx/mapper/mtypeMapper.xml" />	
		<mapper resource="com/stx/mapper/menuMapper.xml" />	
		<mapper resource="com/stx/mapper/puserMapper.xml" />	
		<mapper resource="com/stx/mapper/orderMapper.xml" />	
		
		<!--
		<mapper resource="com/stx/mapper/personMapper.xml" />	
		<mapper resource="com/stx/mapper/teacherMapper.xml" />
		-->
	</mappers>

</configuration>  